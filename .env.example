# Supabase Configuration
# Get these values from your Supabase project dashboard: https://supabase.com/dashboard/project/_/settings/api

# Supabase Project URL (found in Project Settings > API)
NEXT_PUBLIC_SUPABASE_URL=your-project-url.supabase.co

# Supabase Anon/Public Key (found in Project Settings > API)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Supabase Service Role Key (found in Project Settings > API - keep this secret!)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Development redirect URL for email confirmations (use your local dev URL)
NEXT_PUBLIC_DEV_SUPABASE_REDIRECT_URL=http://localhost:3000/dashboard

# Database Connection Strings (found in Project Settings > Database)
SUPABASE_POSTGRES_URL=postgresql://postgres:[password]@[host]:5432/postgres
SUPABASE_POSTGRES_PRISMA_URL=postgresql://postgres:[password]@[host]:5432/postgres?pgbouncer=true&connection_limit=1
SUPABASE_POSTGRES_URL_NON_POOLING=postgresql://postgres:[password]@[host]:5432/postgres

# Additional Supabase Configuration
SUPABASE_JWT_SECRET=your-jwt-secret
SUPABASE_POSTGRES_USER=postgres
SUPABASE_POSTGRES_PASSWORD=your-database-password
SUPABASE_POSTGRES_DATABASE=postgres
SUPABASE_POSTGRES_HOST=your-host.supabase.co
SUPABASE_ANON_KEY=your-anon-key

